  "require": {
    "php": "^7.4 || ^8.0",
    "ext-filter": "*",
    "ext-hash": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpcq/runner-bootstrap": "^1.0@dev"
  },
  "autoload": {
    "psr-4": {
      "Phpcq\\RepositoryDefinition\\": "src/"
